{
  "name": "antdfront",
  "version": "3.0.0",
  "description": "Antdfront is a set of multi tab experimental react microfrontend template",
  "main": "webpack.config.js",
  "scripts": {
    "build": "npx cross-env NODE_ENV=production webpack --mode=production  --config ./webpack/webpack.config.js",
    "dev": "npx cross-env NODE_ENV=development webpack serve  --mode=development --config ./webpack/webpack.config.js",
    "test": "jest",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "prettier": "prettier -c --write \"**/*\""
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --custom-syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write --fix",
      "npm run lint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "keywords": [
    "antd",
    "antd5",
    "recoil",
    "admin",
    "pwa",
    "multitabs",
    "tabs",
    "hook",
    "microfrontend",
    "qiankun",
    "webpack5",
    "webpack",
    "react-router",
    "react",
    "react18",
    "template",
    "scoffold"
  ],
  "devDependencies": {
